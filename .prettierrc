{
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": false,
    "trailingComma": "none",
    "tabWidth": 4,
    "endOfLine": "auto",
    "useTabs": false,
    "singleQuote": false,
    "printWidth": 120,
    "jsxSingleQuote": false,
    "plugins": [
        "prettier-plugin-tailwindcss"
    ]
}